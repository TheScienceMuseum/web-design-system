<div class="c-pagination">
  <ul class="c-pagination__list" role="navigation" aria-label="Pagination">
    {{#if pagination.isFirst}}
    {{else}}
      <li class="c-pagination__previous">
        <a href="{{pagination.links.prev}}" aria-label="Previous">Previous</a>
      </li>
    {{/if}}

    {{#each pagination.links.pages}}
      {{#if this}}
        {{#if this.isCurrent}}
          <li class="current" aria-current="page"><span class="u-visually-hidden">You're on page</span> {{this.pageNumber}}</li>
        {{else}}
          <li><a href="#{{this.pageNumber}}" aria-label="Page {{this.pageNumber}}">{{this.pageNumber}}</a></li>
        {{/if}}
      {{else}}
        <li class="c-pagination__ellipsis"></li>
      {{/if}}
    {{/each}}

    {{#if pagination.isLast}}
    {{else}}
      <li class="c-pagination__next">
        <a href="{{pagination.links.next}}" aria-label="Next">Next</a>
      </li>
    {{/if}}
  </ul>
</div>
